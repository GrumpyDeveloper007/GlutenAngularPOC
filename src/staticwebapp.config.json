{
  "globalHeaders": {
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/places/*",
      "rewrite": "/index.html",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/*",
      "rewrite": "/index.html",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ]
}